{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "types": ["node"],
    "isolatedModules": true,
    "skipLibCheck": true
  }
}
